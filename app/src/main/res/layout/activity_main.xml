<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <com.jym.sandbox.seekbar.view.SubsectionSeekBar
        android:id="@+id/sbSubsection"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:dots="5"
        app:isDisplay="false"
        app:max="100"
        app:progress="0"
        app:thumb="@drawable/bg_subsection_thumb"
        app:unProgressCircleColor="@color/view_subsection_thumb_normal"
        app:unProgressLineColor="@color/view_subsection_background">

    </com.jym.sandbox.seekbar.view.SubsectionSeekBar>

</RelativeLayout>
